<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Description>A small library for comparing published .NET Core directories. Uses the Microsoft.Extensions.DependencyModel library to compare .dll files.</Description>
    <Copyright>@2021 AGreenTejada</Copyright>
    <PackageProjectUrl>https://github.com/agreentejada/filediff</PackageProjectUrl>
    <RepositoryUrl>https://github.com/agreentejada/filediff</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>diff, filecomparer</PackageTags>
    <PackageReleaseNotes>Refactored names.</PackageReleaseNotes>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyModel" Version="5.0.0" />
  </ItemGroup>

</Project>
